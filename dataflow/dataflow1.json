{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "RestService1",
						"type": "LinkedServiceReference"
					},
					"name": "publicBoardsFeed"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "flowlet1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     limit: 100,",
				"     format: 'rest',",
				"     timeout: 30,",
				"     httpMethod: 'GET',",
				"     paginationRules: ['supportRFC5988' -> 'true'],",
				"     responseFormat: ['type' -> 'xml', 'namespaces' -> false]) ~> publicBoardsFeed",
				"derivedColumn1 compose(composition: 'undefined') ~> flowlet1@()",
				"publicBoardsFeed derive() ~> derivedColumn1"
			]
		}
	}
}